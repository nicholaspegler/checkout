{
  "AllowedHosts": "*",
  "Payment": {
    "Bank": {
      "Auth": {
        "Url": "",
        "Key": "",
        "Secret": ""
      }

    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\PaymentGateway.log",
          "retainedFileCountLimit": 30,
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Verbose"
        }
      }
    ]
  }
}