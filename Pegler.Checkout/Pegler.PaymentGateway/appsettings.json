{
  "AllowedHosts": "*",
  "Payment": {
    "Bank": {
      "Authentication": {
        "IsRequired": "false",
        "Url": "",
        "Key": "",
        "Secret": ""
      },
      "Urls": [
        {
          "Type": "Get",
          "Value": "https://localhost:44319/api/v1/Bank/{0}"
        },
        {
          "Type": "Post",
          "Value": "https://localhost:44319/api/v1/Bank/"
        }
      ]
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\PaymentGateway.log",
          "retainedFileCountLimit": 30,
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Verbose"
        }
      }
    ]
  }
}